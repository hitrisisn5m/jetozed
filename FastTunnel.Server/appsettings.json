{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServerSettings": {
    "BindAddr": "0.0.0.0", // 可选
    "BindPort": 1271, // 可选

    "Domain": "test.cc", // 自定义域名web穿透必须

    // 服务监听的端口号, 访问自定义域名站点时url为 http://{SubDomain}.{Domain}:{ProxyPort_HTTP}/
    "ProxyPort_HTTP": 1270, // web穿透必须
    "HasNginxProxy": false // 可选，ngixn反向代理后可省略域名后的端口号进行访问
  }
}